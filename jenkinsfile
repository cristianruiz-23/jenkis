pipeline {
    agent any
    stages {
        stage('Clonar CÃ³digos') {
            steps {
                git 'https://github.com/cristianruiz-23/jenkis.git'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
                junit '**/target/surefire-reports/*.xml'
            }
        }
    }
}
